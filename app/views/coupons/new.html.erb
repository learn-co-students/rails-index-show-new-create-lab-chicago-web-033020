<h1>New Coupon Form</h1>


<%= form_tag("/coupons", method: "post") do %>
  <%= label_tag "coupon[store]", "Store Name:" %>
  <%= text_field_tag "coupon[store]" %>
  <br>
  <%= label_tag 'coupon[coupon_code]', "Coupon Code:" %>
  <%= text_field_tag 'coupon[coupon_code]' %>
  <br>
  <%= submit_tag "Submit Coupon"%>
  <% end %>