<h1>Coupon Form</h1>

<%= form_tag coupons_path do %>
  <label for="coupon_coupon_name">Coupon name: </label>
  <br>
  <%= text_field_tag :'coupon[coupon_code]' %>
  <br>
  <br>
  <label for="coupon_store">Store: </label>
  <%= text_field_tag :'coupon[store]' %>
  <br>
  <br>
  <%= submit_tag "Submit Coupon" %>
<% end %>